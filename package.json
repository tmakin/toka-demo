{
  "name": "toka-demo",
  "version": "1.0.0",
  "description": "STL mesh rendering demo using BabylonJS",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "build:stats": "webpack --config webpack.config.prod.js --profile --json > stats.json",
    "start": "webpack-dev-server --open --config webpack.config.dev.js",
    "deploy": "gh-pages --dist dist",
    "publish": "npm run build && npm run deploy",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "devDependencies": {
    "@babylonjs/core": "4.0.3",
    "@babylonjs/gui": "4.0.3",
    "@babylonjs/loaders": "4.0.3",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.4",
    "gh-pages": "2.0.1",
    "html-webpack-plugin": "3.2.0",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3",
    "webpack": "4.39.1",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.7.2"
  }
}
